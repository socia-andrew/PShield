<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:src="@drawable/globe" />

        <TableRow
            android:id="@+id/row6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/imageView1"
            android:layout_marginTop="15dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/txnstatlbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="10dp"
                android:text="Status:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/txnstatval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="SUCCESS"
                android:textColor="#006DD9" />

        </TableRow>

        <TableRow
            android:id="@+id/row1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row6"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/datelbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:text="Date/Time:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/detailsdatehour"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="20000.00"
                android:textColor="#006DD9"
                android:textSize="13dp" />

        </TableRow>

        <TableRow
            android:id="@+id/row2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row1"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/mobilenolbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="10dp"
                android:text="Mobile No:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/mobilenoval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="20000.00"
                android:textColor="#006DD9"
                android:textSize="13dp" />

        </TableRow>

        <TableRow
            android:id="@+id/row3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row2"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/amountlbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="10dp"
                android:text="Amount:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/amountval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".7"
                android:gravity="left"
                android:text="20000.00"
                android:textColor="#006DD9"
                android:textSize="13dp" />

        </TableRow>

        <TableRow
            android:id="@+id/row4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row3"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff"
          >

            <TextView
                android:id="@+id/desclbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="10dp"
                android:text="Product:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/productCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Sulit Regular Denom 100"
                android:textColor="#006DD9"
                android:textSize="13dp" />

        </TableRow>


        <TableRow
            android:id="@+id/row10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row3"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:lines="2"
                android:paddingLeft="20dp"
                android:paddingRight="10dp"
                android:text="Description:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/productDesc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:singleLine="true"
                android:text="Sulit Regular Denom 100"
                android:textColor="#006DD9"
                android:textSize="13dp" />

        </TableRow>


        <TableRow
            android:id="@+id/row5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row10"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/txnlbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="18dp"
                android:text="TxnNo:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/txnno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="0003120"
                android:textColor="#006DD9" />

        </TableRow>

        <TableRow
            android:id="@+id/row7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row5"
            android:layout_marginTop="20dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/prevballbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="18dp"
                android:text="Pre-Bal:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/prevbalval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="0003120"
                android:textColor="#006DD9" />

        </TableRow>

        <TableRow
            android:id="@+id/row8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row7"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/postballbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="18dp"
                android:text="Post-Bal:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/postbalval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="0003120"
                android:textColor="#006DD9" />

        </TableRow>

        <TableRow
            android:id="@+id/row9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/row8"
            android:layout_marginTop="5dp"
            android:layout_span="2"
            android:background="#ffffff">

            <TextView
                android:id="@+id/discountlbl"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:width="150dp"
                android:gravity="left"
                android:paddingLeft="20dp"
                android:paddingRight="18dp"
                android:text="Discount:"
                android:textColor="#9A9A9A" />

            <TextView
                android:id="@+id/discountval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="0003120"
                android:textColor="#006DD9" />

        </TableRow>

    </LinearLayout>


</RelativeLayout>
